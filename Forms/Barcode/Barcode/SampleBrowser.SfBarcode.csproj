<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.0</TargetFramework>
    <Configurations>Debug;Release;Release-xml</Configurations>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="SampleBrowser.SfBarcode.nuspec" />
    <None Remove="SamplesList\SamplesList.xml" />
	<None Remove="SampleBrowser.SfBarcode.props" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="SamplesList\SamplesList.xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="SampleBrowser.Core" Version="17.1.0.38" />
    <PackageReference Include="Syncfusion.Xamarin.Core" Version="17.1.0.38" />
    <PackageReference Include="Syncfusion.Xamarin.DataSource" Version="17.1.0.38" />
    <PackageReference Include="Syncfusion.Xamarin.GridCommon" Version="17.1.0.38" />
    <PackageReference Include="Syncfusion.Xamarin.SfBarcode" Version="17.1.0.38" />
    <PackageReference Include="Syncfusion.Xamarin.SfListView" Version="17.1.0.38" />
    <PackageReference Include="Xamarin.Forms" Version="3.2.0.871581" />
	 <EmbeddedResource Include="Syncfusion.lic" />
  </ItemGroup>  
  <ItemGroup>
    <Compile Condition=" '$(EnableDefaultCompileItems)' == 'true' " Update="App.xaml.cs">
      <DependentUpon>*.xaml</DependentUpon>
    </Compile>
    <Compile Condition=" '$(EnableDefaultCompileItems)' == 'true' " Update="MainPage.xaml.cs">
      <DependentUpon>*.xaml</DependentUpon>
    </Compile>
  </ItemGroup>
  <Import Project="..\..\..\..\build\tools\Syncfusion.CodeAnalysis.StyleCop\tools\StyleCop.MsBuild.targets" Condition="Exists('..\..\..\..\build\tools\Syncfusion.CodeAnalysis.StyleCop\tools\StyleCop.MsBuild.targets')" />
</Project>